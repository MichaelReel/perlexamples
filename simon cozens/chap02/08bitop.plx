#!/usr/bin/perl
use warnings;

#bitop1.plx
print "51 ANDed with 85 gives us ", 51 & 85, "\n";
print "51 ORed with 85 gives us ", 51 | 85, "\n";
print "51 XORed with 85 gives us ", 51 ^ 85, "\n";
print "---\n";

#bitop2.plx
print "NOT 85 is ", ~85, "\n";
print "NOT 51 is ", ~51, "\n";
print "---\n";

